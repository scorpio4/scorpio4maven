@prefix rdf:		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:		<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:		<http://www.w3.org/2002/07/owl#> .
@prefix dc:			<http://purl.org/dc/elements/1.1/> .
@prefix eg:			<http://www.example.org/> .
@prefix xsd: 		<http://www.w3.org/2001/XMLSchema#>.

@prefix iq: 		<urn:factcore:>.
@prefix script:		<urn:factcore:script:>.
@prefix fact:		<urn:factcore:fact:>.
@prefix : 			<urn:factcore:application:factcore:iq:>.

#
# IQ Behaviours
#

:exampleAwareness
	a			iq:Example;
	rdfs:label	"Example Awareness";
	:triggers	<urn:factcore:application:factcore:iq:HelloWorld.groovy>;

	iq:exhibits	:exampleSPARQLAbility;
.

:exampleSPARQLAbility
	rdfs:label	"Example SPARQL Ability";
	iq:classname	"com.factcore.iq.ability.sparql.SPARQLAbility";
.

:examplePlanner
	a			iq:Example;
	rdfs:label	"Example Planner";
	:plans		:examplePlan;
	:plans		:timedPlan;
.

:examplePlan
	a			iq:Example;
	rdfs:label	"Example Plan";
	:for		[
		:query "SELECT DISTINCT ?this WHERE {?this a iq:Application}";
	];
	:from 		<direct://{{this}}>;
	:to			<bean:com.factcore.vendor.camel.IQPlannerTest?method=testPlanCallback>;
.

:timedPlan
	a			iq:Example;
	rdfs:label	"Example Timed Plan";
	:for		[
		:query "SELECT DISTINCT ?this WHERE {?this a iq:Application}";
	];
	:from 		<timer://jdkTimer?period=2000>;
	:to			<bean:com.factcore.vendor.camel.IQPlannerTest?method=testTimerCallback>;
.

<bean:com.factcore.vendor.camel.IQPlannerTest?method=testPlanCallback>
	rdfs:label	"Example Camel Route Two";
	:to			<bean:com.factcore.vendor.camel.IQPlannerTest?method=testPlan2Callback>;
.

<bean:com.factcore.vendor.camel.IQPlannerTest?method=testPlan2Callback>
	rdfs:label	"Example Camel Route Three";
.

<urn:factcore:application:factcore:iq:HelloWorld>
	a			iq:Example;
	a			fact:SPARQL;
	rdfs:label	"Hello World SPARQL";
	:script		"SELECT DISTINCT ?this ?label WHERE { ?this a iq:Example;rdfs:label ?label }";
.

<urn:factcore:application:factcore:iq:HelloWorld>
	a			iq:Example;
	a			:Groovy;
	rdfs:label	"Hello World Groovy";
	:script		"println 'hello world';";
.

<urn:factcore:application:factcore:iq:AllScripts>
	a			iq:Example;
	a			:SPARQL;
	rdfs:label	"Hello World SPARQL";
	:script		"SELECT DISTINCT ?this ?label WHERE { ?this a iq:ScriptedBehaviour;rdfs:label ?label }";
.

