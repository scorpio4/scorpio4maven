@prefix rdf:		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:		<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:		<http://www.w3.org/2002/07/owl#> .
@prefix dc:			<http://purl.org/dc/elements/1.1/> .
@prefix eg:			<http://www.example.org/> .
@prefix xsd: 		<http://www.w3.org/2001/XMLSchema#>.

@prefix iq: 		<urn:factcore:>.
@prefix fact:		<urn:factcore:fact:>.
@prefix can:		<urn:factcore:can:>.
@prefix script:		<urn:factcore:script:>.
@prefix asq:		<urn:factcore:asq:>.
@prefix nms:	 	<urn:factcore:nms:>.
@prefix grant:	 	<urn:factcore:grant:>.
@prefix ux:	 		<urn:factcore:ux:>.
@prefix user:	 	<urn:factcore:user:>.

@prefix : 			<urn:factcore:application:factcore:iq:ability:>.

:
    grant:this      :SPARQL;
    grant:this      :SQL;
    grant:this      :Camel;
    grant:this      :Activiti;

    grant:to        <urn:factcore:application:factcore:>;
    grant:from      <urn:factcore:application:factcore:>;
.

:SPARQL
    a               iq:Ability;
    rdfs:label      "SPARQL Ability";
    iq:classname    "com.factcore.iq.ability.sparql.SPARQLAbility";
	iq:enabled      "true"^^xsd:boolean;
.

:SQL
    a               iq:Ability;
    rdfs:label      "SQL Ability";
    iq:classname    "com.factcore.iq.ability.sql.SQLAbility";
	iq:enabled      "false"^^xsd:boolean;
.

:Camel
    a               iq:Ability;
    rdfs:label      "Camel Ability";
    iq:classname    "com.factcore.iq.ability.camel.CamelAbility";
	iq:enabled      "true"^^xsd:boolean;
.

:Script
    a               iq:Ability;
    rdfs:label      "Script Ability";
    iq:classname    "com.factcore.iq.ability.camel.ScriptAbility";
	iq:enabled      "true"^^xsd:boolean;
.

:Activiti
    a               iq:Ability;
    rdfs:label      "Activiti BPM Ability";
    iq:classname    "com.factcore.iq.ability.bpm.ActivitiAbility";
	iq:enabled      "false"^^xsd:boolean;
.
